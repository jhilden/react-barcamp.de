{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "kitconcept Team <info@kitconcept.com>",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "gatsby": "^2.0.66",
    "gatsby-image": "^2.0.23",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-offline": "^2.0.19",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-sharp": "^2.1.9",
    "google-map-react": "^1.1.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "cd public && python -m SimpleHTTPServer 8000",
    "format": "prettier --write \"src/**/*.js\"",
    "format:ci": "prettier --list-different \"src/**/*.js\"",
    "psi": "psi https://react-barcamp.de --threshold 8 ; true",
    "lighthouse": "node_modules/lighthouse/lighthouse-cli/index.js --output-path=./lighthouse-report.html --quiet --chrome-flags='--headless' https://react-barcamp.de",
    "lighthouse:ci": "node_modules/lighthouse/lighthouse-cli/index.js --output-path=./lighthouse-report.html --quiet --chrome-flags='--headless' https://react-barcamp.de",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "7.4.0",
    "lighthouse": "^4.0.0-alpha.2-3.2.1",
    "prettier": "^1.15.3",
    "prettier-stylelint": "^0.4.2",
    "psi": "^3.1.0",
    "stylelint": "^9.5.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-idiomatic-order",
      "./node_modules/prettier-stylelint/config.js"
    ]
  }
}
